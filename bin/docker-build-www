#!/usr/bin/env bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"

set -ex

TAG=docker.repo1.uhc.com/optumassistant/ui-components:latest

if [[ ! -z "$LOGIN" ]]; then
  docker login docker.repo1.uhc.com --username=$DOCKER_USERNAME --password=$DOCKER_PASSWORD
fi

docker build -f Dockerfile-www -t $TAG . && docker push $TAG
