  "types": "build/index.d.ts",
  "description": "A skeleton to create your own React component library using Rollup, TypeScript, Sass and Storybook",
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -s ./public -p 6007",
    "storybook:export": "build-storybook -s ./public",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build",
    "publish_gh_pages": "rm -rf storybook-static/.git && cd storybook-static && git init && git add . && git commit -m \"Initial commit\" && git remote add github git@github.optum.com:SofaSurfers/ui-components.git && git push --force github master:gh-pages"
  },
